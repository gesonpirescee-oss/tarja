    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "multer": "^1.4.5-lts.1",
    "bullmq": "^5.3.0",
    "ioredis": "^5.3.2",
